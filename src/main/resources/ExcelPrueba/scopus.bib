@CONFERENCE{Pillai2017833,
author={Pillai, S.P. and Madhukumar, S.D. and Radharamanan, T.},
title={Consolidating evidence based studies in software cost/effort estimation - A tertiary study},
journal={IEEE Region 10 Annual International Conference, Proceedings/TENCON},
year={2017},
volume={2017-December},
pages={833-838},
doi={10.1109/TENCON.2017.8227974},
